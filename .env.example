# The URL to the CMS, without the path (e.g. https://instance_id.cms.optimizely.com/)
OPTIMIZELY_CMS_URL=
OPTIMIZELY_CMS_CLIENT_ID=
OPTIMIZELY_CMS_CLIENT_SECRET=

# The domain where the frontend will run (e.g. your-project.vercel.app), only set this
# if the detection fails
# SITE_DOMAIN=$VERCEL_BRANCH_URL

# Optimizely Graph Keys
OPTIMIZELY_GRAPH_SECRET=
OPTIMIZELY_GRAPH_APP_KEY=
OPTIMIZELY_GRAPH_SINGLE_KEY=
OPTIMIZELY_GRAPH_GATEWAY=https://cg.optimizely.com
OPTIMIZELY_GRAPH_UPDATE_DELAY=2000
OPTIMIZELY_GRAPH_QUERY_LOG=0

# Configure Optimizely context
OPTIMIZELY_DEBUG=0
# Arbitrary publish token - needs to be a secure value
OPTIMIZELY_PUBLISH_TOKEN=

# One of `mshealth`, `icahn`, or `nyee`
OPTIMIZELY_CHANNEL=mshealth

# Change based on channel
NEXT_PUBLIC_DOMAIN_URL=https://www.mountsinai.org
